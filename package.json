{
  "name": "nvdb-vis-vegobjekter",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --hot ./server.ts",
    "build": "bun build ./index.html --outdir ./dist",
    "fetch:specs": "bun run fetch:datakatalog && bun run fetch:uberiket",
    "fetch:datakatalog": "curl -sS -H 'X-Client: nvdb-vis-vegobjekter' 'https://nvdbapiles.atlas.vegvesen.no/api-docs/datakatalog' -o specs/datakatalog.json",
    "fetch:uberiket": "curl -sS -H 'X-Client: nvdb-vis-vegobjekter' 'https://nvdbapiles.atlas.vegvesen.no/api-docs/uberiket' -o specs/uberiket.json",
    "generate:api": "bunx openapi-ts",
    "typecheck": "bunx tsc --noEmit"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.91.0",
    "@types/bun": "latest",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.19",
    "axios": "^1.13.2",
    "jotai": "^2.16.2",
    "ol": "^10.7.0",
    "proj4": "^2.20.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "zod": "^4.3.5"
  }
}
